{
    "name": "keboola/output-mapping",
    "description": "Shared component for processing SAPI output mapping and importing data to KBC",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Keboola",
            "email": "devel@keboola.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Keboola\\OutputMapping\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keboola\\OutputMapping\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "libs": {
            "type": "path",
            "url": "../../libs/*"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "keboola/input-mapping": "*@dev",
        "keboola/php-datatypes": "^6.0",
        "keboola/php-file-storage-utils": "^0.2",
        "keboola/sanitizer": "^0.1",
        "keboola/storage-api-client": "^14.11.1",
        "keboola/storage-api-php-client-branch-wrapper": "^4.0",
        "microsoft/azure-storage-blob": "^1.5",
        "monolog/monolog": "^1.25.5|^2.0",
        "symfony/config": "^5.4|^6.0",
        "symfony/finder": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0"
    },
    "require-dev": {
        "keboola/coding-standard": ">=14.0",
        "keboola/php-temp": "^2.0",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4",
        "symfony/dotenv": "^5.4|^6.0"
    },
    "scripts": {
        "pre-autoload-dump": "Aws\\Script\\Composer\\Composer::removeUnusedServices",
        "tests": "phpunit",
        "phpstan": "phpstan analyse --no-progress -c phpstan.neon",
        "phpcs": "phpcs --extensions=php src tests",
        "phpcbf": "phpcbf --extensions=php src tests",
        "check": [
            "@composer validate --no-check-all --strict",
            "@phpcs",
            "@phpstan"
        ],
        "ci": [
            "@check",
            "@tests"
        ]
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 3600,
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "aws/aws-sdk-php": [
            "S3",
            "Kms",
            "Sts"
        ]
    }
}
